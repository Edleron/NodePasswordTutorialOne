<div class="card">
    <h5 class="card-header"> Registered Users </h5>

    <div class="card-body">
        <ul class="list-group">
            {{#each users}}
            <li class="li list-group-item">
                {{username}}
            </li>
            {{else}}
            <div class="alert-info">
                There is No Registered Users!
            </div>
            {{/each}}
        </ul>
    </div>
</div>

{{#if user}}
<h4 class="m-2">Aktif Kullanıcı : {{user.username}}</h4>
{{/if}}